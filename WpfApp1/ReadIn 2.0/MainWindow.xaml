<Window x:Class="ReadIn_2._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReadIn_2._0"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="bttnEnter" Content="Enter" HorizontalAlignment="Left" Margin="400,119,0,0" VerticalAlignment="Top" Height="35" Width="120" Click="Button_Click"/>
        <TextBox x:Name="txtlink" HorizontalAlignment="Left" Margin="100,128,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="255" Height="18"/>
        <ListBox x:Name="lstBox" Margin="0,174,0,0"/>
        <Button x:Name="bttnWrite" Content="Write" HorizontalAlignment="Left" Margin="560,116,0,0" VerticalAlignment="Top" Height="41" Width="120" Click="bttnWrite_Click"/>

    </Grid>
</Window>
